{
  "compilerOptions": {
    "outDir": "dist",
    "strict": true,
    "target": "es2020",
    "moduleResolution": "node10",
    "module": "commonjs",
    "isolatedModules": true,
    "noEmit": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true
  },
  "include": ["src"],
  "exclude": ["dist", "node_modules"]
}
